<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:sl="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="120dp"
    xmlns:whatever="http://schemas.android.com/apk/res-auto"
    xmlns:sw="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="horizontal"
    android:id="@+id/listItem"
    tools:context=".Constant">
    <ImageView
        android:id="@+id/ivImage"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="1dp"
        android:background="@drawable/join_agency_image"/>
    <LinearLayout
        android:layout_gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="7dp"
        android:orientation="vertical">
        <TextView
            android:id="@+id/txtTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="#444444"
            android:text="room_regi_type"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/txtLocate"
            android:layout_marginTop="2dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13sp"
            android:textColor="#777777"
            android:text="room_regi_type"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/txtLocateDetail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13sp"
            android:textColor="#444444"
            android:text="room_regi_type"
            android:gravity="center_vertical"
            />
        <ImageView
            android:id="@+id/ivType"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:background="@drawable/list_user_off"
            />
    </LinearLayout>
</LinearLayout>

