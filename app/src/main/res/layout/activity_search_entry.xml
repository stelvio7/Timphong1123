<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:sl="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:whatever="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    tools:context=".Constant">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                sl:scale_base_width="100"
                sl:scale_base_height="14"
                android:gravity="center"
                android:orientation="vertical">
                <Button
                    android:id="@+id/btnBack"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    sl:scale_left="0"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="14"
                    android:background="@drawable/btnback_entry"></Button>
            </com.nhm.timphong.util.ScalableLayout>
            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_above="@+id/main_textview"
                sl:scale_base_width="400"
                sl:scale_base_height="70">
                <RadioGroup
                    android:layout_marginLeft="7dp"
                    android:id="@+id/radioGroupClass"
                    android:gravity="center"
                    sl:scale_left="0"
                    sl:scale_top="14"
                    sl:scale_width="400"
                    sl:scale_height="43"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checkedButton="@+id/radioSworder"
                    android:orientation="horizontal">
                    <RadioButton
                        android:id="@+id/radioSworder"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="#484848"
                        android:button="@null"
                        android:background="@drawable/entry_search_title_off"
                        />
                    <RadioButton
                        android:id="@+id/radioGunner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="#484848"
                        android:button="@null"
                        android:background="@drawable/area_search_title_off"></RadioButton>
                </RadioGroup>
            </com.nhm.timphong.util.ScalableLayout>

            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                sl:scale_base_width="400"
                sl:scale_base_height="50"
                android:gravity="center"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    sl:scale_left="0"
                    sl:scale_top="0"
                    sl:scale_width="400"
                    sl:scale_height="50"
                    android:background="@drawable/area_point"/>
            </com.nhm.timphong.util.ScalableLayout>
            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                sl:scale_base_width="200"
                sl:scale_base_height="25"
                android:gravity="center"
                android:orientation="vertical">
                <Button
                    android:id="@+id/btnAreaBack3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="0"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
                <Button
                    android:id="@+id/btnAreaBack4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="100"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
            </com.nhm.timphong.util.ScalableLayout>
            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                sl:scale_base_width="200"
                sl:scale_base_height="25"
                android:gravity="center"
                android:orientation="vertical">
                <Button
                    android:id="@+id/btnAreaBack5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="0"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
                <Button
                    android:id="@+id/btnAreaBack6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="100"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
            </com.nhm.timphong.util.ScalableLayout>
            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                sl:scale_base_width="200"
                sl:scale_base_height="25"
                android:gravity="center"
                android:orientation="vertical">
                <Button
                    android:id="@+id/btnAreaBack7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="0"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
                <Button
                    android:id="@+id/btnAreaBack8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="100"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
            </com.nhm.timphong.util.ScalableLayout>
            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                sl:scale_base_width="200"
                sl:scale_base_height="25"
                android:gravity="center"
                android:orientation="vertical">
                <Button
                    android:id="@+id/btnAreaBack9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="0"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
                <Button
                    android:id="@+id/btnAreaBack10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="100"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
            </com.nhm.timphong.util.ScalableLayout>
            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                sl:scale_base_width="200"
                sl:scale_base_height="25"
                android:gravity="center"
                android:orientation="vertical">
                <Button
                    android:id="@+id/btnAreaBack11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="0"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
                <Button
                    android:id="@+id/btnAreaBack12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="100"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
            </com.nhm.timphong.util.ScalableLayout>
            <com.nhm.timphong.util.ScalableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                sl:scale_base_width="200"
                sl:scale_base_height="25"
                android:gravity="center"
                android:orientation="vertical">
                <Button
                    android:id="@+id/btnAreaBack13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="0"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
                <Button
                    android:id="@+id/btnAreaBack14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    sl:scale_left="100"
                    sl:scale_top="0"
                    sl:scale_width="100"
                    sl:scale_height="25"
                    android:layout_weight="1"
                    android:background="@drawable/btn_area_left_back"
                    />
            </com.nhm.timphong.util.ScalableLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
