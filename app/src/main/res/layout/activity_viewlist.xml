<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:sl="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:whatever="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	tools:context=".Constant">
	<com.nhm.timphong.util.ScalableLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		sl:scale_base_width="100"
		sl:scale_base_height="15"
		android:gravity="center"
		android:orientation="vertical">
		<Button
			android:id="@+id/btnBack"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			sl:scale_left="0"
			sl:scale_top="0"
			sl:scale_width="85"
			sl:scale_height="15"
			android:background="@drawable/btnback_mapsearchresult"></Button>
		<Button
			android:id="@+id/btnMapFilter"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			sl:scale_left="85"
			sl:scale_top="0"
			sl:scale_width="15"
			sl:scale_height="15"
			android:background="@drawable/btn_mapfinlter"></Button>
	</com.nhm.timphong.util.ScalableLayout>
	<fragment
		android:id="@+id/map"
		android:layout_weight="1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:name="com.google.android.gms.maps.SupportMapFragment"
		class="com.google.android.gms.maps.SupportMapFragment"/>
</LinearLayout>
