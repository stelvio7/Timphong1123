<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="0dip" />
			<gradient android:startColor="#FFFFFF"
			android:endColor="#FFFFFF" android:angle="270" />
			<stroke android:width="1px" android:color="#FFFFFF" />
		</shape>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="0dip" />
				<gradient android:startColor="#43B5CF"
				android:endColor="#43B5CF" android:angle="270" />
				<stroke android:width="1px" android:color="#43B5CF"/>
			</shape>
		</clip>
	</item>
</layer-list>
